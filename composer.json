{
    "name": "nutandc/api-crud-generator",
    "type": "library",
    "description": "Laravel API CRUD generator for models, controllers, requests, resources, migrations, routes, plus optional repository/service patterns.",
    "license": "MIT",
    "homepage": "https://github.com/Nutandc/laravel-ApiCrud-Generator",
    "keywords": [
        "Laravel",
        "CRUD",
        "API",
        "Generator",
        "Scaffold"
    ],
    "autoload": {
        "psr-4": {
            "Nutandc\\ApiCrud\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Nutan Dahal",
            "email": "info@dahalnutan.com.np"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "illuminate/filesystem": "^10.0|^11.0|^12.0",
        "illuminate/routing": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpstan/phpstan": "^1.11",
        "phpstan/phpstan-phpunit": "^1.4",
        "friendsofphp/php-cs-fixer": "^3.92"
    },
    "scripts": {
        "test": "phpunit",
        "analyse": "phpstan analyse --configuration=phpstan.neon",
        "fix": "php-cs-fixer fix",
        "fix:dry": "php-cs-fixer fix --dry-run --diff"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Nutandc\\ApiCrud\\Providers\\ApiCrudGeneratorServiceProvider"
            ]
        }
    }
}
